@article{satyanarayan2016,
  title = {Reactive {{Vega}}: {{A}} Streaming Data Flow Architecture for Declarative Interactive Visualization},
  author = {Satyanarayan, Arvind and Russell, Ryan and Hoffswell, Jane and Heer, Jeffrey},
